{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.23.2",
  "pnpmVersion": "2.15.1",
  "pnpmOptions": {
    "strictPeerDependencies": true
  },

  "nodeSupportedVersionRange": ">=10.13.0 <11.0.0",
  "ensureConsistentVersions": true,

  "approvedPackagesPolicy": {
    "reviewCategories": ["production", "tools", "prototypes"],

    "ignoredNpmScopes": ["@types"]
  },

  "gitPolicy": {},

  "repository": {
    "url": "https://github.com/DTCurrie/sr5e-tools",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },

  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },

  "variants": [],

  "telemetryEnabled": false,

  "projects": [
    {
      "packageName": "run-generator",
      "projectFolder": "apps/run-generator",
      "reviewCategory": "production",
      "cyclicDependencyProjects": []
    },
    {
      "packageName": "game-mechanics",
      "projectFolder": "tools/game-mechanics",
      "reviewCategory": "tools",
      "cyclicDependencyProjects": []
    }
  ]
}
